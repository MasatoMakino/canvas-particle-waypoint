{
  "name": "@masatomakino/canvas-particle-waypoint",
  "version": "0.5.5",
  "description": "Particle waypoint module for canvas 2d",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./esm/index.js",
  "files": [
    "lib",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://MasatoMakino@github.com/MasatoMakino/canvas-particle-waypoint.git"
  },
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@masatomakino/particle-waypoint": "^0.5.4"
  },
  "devDependencies": {
    "@masatomakino/gulptask-demo-page": "^0.5.4",
    "@tweenjs/tween.js": "^18.6.4",
    "browser-sync": "^2.27.11",
    "husky": "^8.0.3",
    "lil-gui": "^0.18.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.25",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "prepare": "husky install",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run buildTS && npm run demo && npm run typedocs",
    "demo": "npx @masatomakino/gulptask-demo-page",
    "buildTS": "tsc --project tsconfig.json & tsc --project tsconfig.esm.json",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "server": "npx browser-sync ./docs/demo -w",
    "watch:demo": "npm run demo -- -W",
    "watch:tsc": "tsc -W --incremental --project tsconfig.json & tsc -W --incremental --project tsconfig.esm.json"
  },
  "bugs": {
    "url": "https://github.com/MasatoMakino/canvas-particle-waypoint/issues"
  },
  "homepage": "https://github.com/MasatoMakino/canvas-particle-waypoint#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
